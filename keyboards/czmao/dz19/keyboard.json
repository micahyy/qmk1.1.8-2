{
    "keyboard_name": "dz19",
    "manufacturer": "CZMAO",
    "url": "https://github.com/micahyy/czmao/tree/main/ds17_solder",
    "maintainer": "MAOKB",
    "diode_direction": "COL2ROW",
    "processor": "STM32F103",
    "bootloader": "stm32duino",
    "usb": {
      "vid": "0x4A99",
      "pid": "0x4A99",
      "device_version": "1.0.0"
    },
    "features": {
        "bootmagic": true,
        "command": true,
        "console": true,
        "extrakey": true,
    },
    "dynamic_keymap": {
      "layer_count": 8
    },
    

    
    
    "matrix_pins": {
      "cols": ["B12", "B14", "B15", "A8"],
      "rows": ["A9", "A10", "A15",  "B3",  "B4"]
    },

    "layouts": {
      "LAYOUT_numberpad_5x4": {
        "layout": [
          {"label":"F1",    "matrix": [ 0,0],"x":0,"y":0},
          {"label":"UP",    "matrix": [ 0,1],"x":1,"y":0},
          {"label":"DOWN",  "matrix": [ 0,2],"x":2,"y":0},
          {"label":"F2",    "matrix": [ 0,3],"x":3,"y":0},
          
          {"label":"P7",    "matrix": [ 1,0],"x":0,"y":1},
          {"label":"P8",    "matrix": [ 1,1],"x":1,"y":1},
          {"label":"P9",    "matrix": [ 1,2],"x":2,"y":1},
          {"label":"DEL",   "matrix": [ 1,3],"x":2,"y":1},

          {"label":"P4",    "matrix": [ 2,0],"x":2,"y":1},
          {"label":"P5",    "matrix": [ 2,1],"x":3,"y":1},
          {"label":"P6",    "matrix": [ 2,2],"x":4,"y":1},
          {"label":"ESC",   "matrix": [ 2,3],"x":3,"y":1,"h":2},
          
          {"label":"P1",    "matrix": [ 3,0],"x":0,"y":2},
          {"label":"P2",    "matrix": [ 3,1],"x":1,"y":2},
          {"label":"P3",    "matrix": [ 3,2],"x":2,"y":2},
          
          {"label":"P0",    "matrix": [ 4,0],"x":0,"y":3},
          {"label":"PDOT",  "matrix": [ 4,1],"x":1,"y":3},
          {"label":"P+",    "matrix": [ 4,2],"x":3,"y":2,"h":2},
          {"label":"P9",    "matrix": [ 4,3],"x":2,"y":1}

          
          

        ]
      }
    }

  }
