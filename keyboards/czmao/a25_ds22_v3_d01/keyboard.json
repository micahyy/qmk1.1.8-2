{
    "keyboard_name": "CZMAO DS22R3",
    "manufacturer": "CZMAO",
    "url": "https://github.com/micahyy/czmao/tree/main/ds17_solder",
    "maintainer": "MAOKB",
    "diode_direction": "COL2ROW",
    "processor": "STM32F103",
    "bootloader": "stm32duino",
    "usb": {
      "vid": "0x4A25",
      "pid": "0x4A26",
      "device_version": "1.0.0",
      "force_nkro": true
    },
    "features": {
        "bootmagic": true,
        "nkro": true,
        "command": true,
        "extrakey": true,
        "rgb_matrix": true
    },
    "dynamic_keymap": {
      "layer_count": 8
    },

    "matrix_pins": {
        "cols": ["B12", "B14", "B15", "A8", "A9"],
        "rows": ["C13", "C14", "A0",  "A1",  "A2"]
      },

      "ws2812": {
        "pin": "B13"
      },

      "rgb_matrix": {
        "animations": {
            "gradient_left_right": true,
            "breathing": true,
            "band_sat": true,
            "band_val": true,
            "band_pinwheel_sat": true,
            "band_pinwheel_val": true,
            "band_spiral_sat": true,
            "band_spiral_val": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "rainbow_pinwheels": true,
            "raindrops": true,
            "jellybean_raindrops": true,
            "hue_breathing": true,
            "hue_pendulum": true,
            "hue_wave": true,
            "typing_heatmap": true,
            "solid_reactive_simple": true,
            "solid_reactive": true,
            "solid_reactive_wide": true,
            "solid_reactive_multiwide": true,
            "solid_reactive_cross": true,
            "solid_reactive_multicross": true,
            "solid_reactive_nexus": true,
            "solid_reactive_multinexus": true,
            "splash": true,
            "multisplash": true,
            "solid_splash": true,
            "solid_multisplash": true
        },
        "driver": "ws2812",
        "max_brightness":180,
        "default":{
          "animation":"cycle_left_right"
          },
        "layout": [
            { "flags": 2, "matrix": [0, 0], "x": 0, "y": 0 },
            { "flags": 2, "matrix": [0, 1], "x": 56, "y": 0 },
            { "flags": 2, "matrix": [0, 2], "x": 112, "y": 0 },
            { "flags": 2, "matrix": [0, 3], "x": 168, "y": 0 },
            { "flags": 2, "matrix": [0, 4], "x": 224, "y": 0 },

            { "flags": 2, "matrix": [1, 0], "x": 0, "y":  16},
            { "flags": 2, "matrix": [1, 1], "x": 56, "y": 16 },
            { "flags": 2, "matrix": [1, 2], "x": 112, "y": 16 },
            { "flags": 2, "matrix": [1, 3], "x": 168, "y": 16 },

            { "flags": 2, "matrix": [2, 0], "x": 0, "y": 32 },
            { "flags": 2, "matrix": [2, 1], "x": 56, "y": 32 },
            { "flags": 2, "matrix": [2, 2], "x": 112, "y": 32 },
            { "flags": 2, "matrix": [2, 3], "x": 168, "y": 32 },
            { "flags": 2, "matrix": [2, 4], "x": 224, "y": 40 },

            { "flags": 2, "matrix": [3, 0], "x": 0, "y": 48 },
            { "flags": 2, "matrix": [3, 1], "x": 56, "y": 48 },
            { "flags": 2, "matrix": [3, 2], "x": 112, "y": 48 },
            { "flags": 2, "matrix": [3, 3], "x": 168, "y": 48 },


            { "flags": 2, "matrix": [4, 0], "x": 0, "y": 64 },
            { "flags": 2, "matrix": [4, 1], "x": 84, "y": 64 },
            { "flags": 2, "matrix": [4, 2], "x": 168, "y": 64 },
            { "flags": 2, "matrix": [4, 4], "x": 224, "y": 56 }
            ],
            "sleep": true
        },
    "layouts": {
        "LAYOUT_numpad_5x5": {
            "layout": [
                {"label":"DEL",    "matrix": [ 0,0],"x":0,   "y":0},
                {"label":"NUM",   "matrix": [ 0,1],"x":1.25,"y":0},
                {"label":"PSLS",  "matrix": [ 0,2],"x":2.25,"y":0},
                {"label":"PAST",  "matrix": [ 0,3],"x":3.25,"y":0},
                {"label":"PMNS",  "matrix": [ 0,4],"x":4.25,"y":0},

                {"label":"HOME",  "matrix": [ 1,0],"x":0,   "y":1},
                {"label":"P7",    "matrix": [ 1,1],"x":1.25,"y":1},
                {"label":"P8",    "matrix": [ 1,2],"x":2.25,"y":1},
                {"label":"P9",    "matrix": [ 1,3],"x":3.25,"y":1},

                {"label":"PGUP",  "matrix": [2,0],"x":0,   "y":2},
                {"label":"P4",    "matrix": [2,1],"x":1.25,"y":2},
                {"label":"P5",    "matrix": [2,2],"x":2.25,"y":2},
                {"label":"P6",    "matrix": [2,3],"x":3.25,"y":2},
                {"label":"PPLS",  "matrix": [2,4],"x":4.25,"y":2,"h":2},

                {"label":"PGDN",  "matrix": [ 3,0],"x":0,   "y":3},
                {"label":"P1",    "matrix": [ 3,1],"x":1.25,"y":3},
                {"label":"P2",    "matrix": [ 3,2],"x":2.25,"y":3},
                {"label":"P3",    "matrix": [ 3,3],"x":3.25,"y":3},

                {"label":"FN",    "matrix": [ 4,0],"x":0,   "y":4},
                {"label":"P0",    "matrix": [ 4,1],"x":1.25,"y":4},
                {"label":"PDOT",  "matrix": [ 4,2],"x":2.25,"y":4},
                {"label":"PENT",  "matrix": [ 4,4],"x":3.25,"y":4,"h":2}

            ]
        }
    }
}

